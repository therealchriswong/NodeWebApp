doctype 5
html
    
    include header/header

    body
        div#wrap
            //Fixed navbar
            div.navbar.navbar-inverse.navbar-fixed-top(role="navigation")
                div.container
                    div.navbar-header
                        button.navbar-toggle( type="button" data-toggle="collapse" data-target=".navbar-collapse")
                            span.sr-only Toggle navigation
                            span.icon-bar
                            span.icon-bar
                            span.icon-bar
                        a(href='/').navbar-brand Project Name
                    div.collapse.navbar-collapse
                        ul.nav.navbar-nav
                            - var obj = { '/':'Home', 'helloworld':'Hello World!', 'userlist':'User List', 'newuser':'New User'}
                            each val, key in obj
                                if (id == key)
                                    li.active
                                        a(href='#{key}') #{val}
                                else            
                                    li
                                        a(href='#{key}') #{val}
            //Begin page content
            block content

        include footer/footer
        // Bootstrap core Javascript
        include scripts/scripts



 